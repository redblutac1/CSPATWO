<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cs5132_patwo.MyCompoundsController">
   <children>
      <Label layoutX="236.0" layoutY="14.0" text="My Compounds">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" layoutY="92.0" prefHeight="40.0" prefWidth="600.0" spacing="20.0">
         <children>
            <Button fx:id="addCompoundButton" mnemonicParsing="false" onAction="#addCompound" text="Add Compound" />
            <Button fx:id="exploreButton" mnemonicParsing="false" onAction="#explore" text="Explore" />
         </children>
      </HBox>
      <ListView fx:id="compoundsListView" layoutX="46.0" layoutY="140.0" prefHeight="246.0" prefWidth="507.0" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="15.0" mnemonicParsing="false" onAction="#returnToHome" text="Back" />
      <TextField fx:id="compoundTextField" layoutX="129.0" layoutY="66.0" prefHeight="25.0" prefWidth="341.0" promptText="Enter name and reactants here, separated with | characters" />
   </children>
</AnchorPane>
